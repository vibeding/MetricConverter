{
  "manifest_version": 3,
  "name": "Metric/Imperial Converter",
  "version": "1.0",
  "description": "Convert numbers between Metric and Imperial units easily.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/logo-16.png",
      "48": "icons/logo-48.png",
      "128": "icons/logo-128.png"
    }
  },
  "icons": {
    "16": "icons/logo-16.png",
    "48": "icons/logo-48.png",
    "128": "icons/logo-128.png"
  }
}